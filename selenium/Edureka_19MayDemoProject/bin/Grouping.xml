<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Grouping Example">
	<test name="Grouping Test">
		<groups>
			<run>
				<include name="ContactUs"></include>
				<exclude name="HomePage"></exclude>
			</run>
		</groups>
		<classes>
			<class name="Module5.Grouping"></class>
		</classes>
	</test>
</suite>

